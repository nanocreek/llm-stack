# Environment variables for OpenWebUI service
# These will be set automatically by Railway using service references

# OpenWebUI Server Configuration
PORT=8080

# LiteLLM Configuration - Connection to LiteLLM Service
# OpenWebUI uses OpenAI-compatible API endpoint from LiteLLM
OPENAI_API_BASE_URL=http://litellm.railway.internal:4000/v1
OPENAI_API_KEY=${{LiteLLM.LITELLM_MASTER_KEY}}

# PostgreSQL Configuration - Connection to Railway PostgreSQL Plugin
# Used for storing user data, conversations, and chat history
DATABASE_URL=${{Postgres.DATABASE_URL}}

# Redis Configuration - Connection to Railway Redis Plugin
# Used for caching and session management
REDIS_URL=${{Redis.REDIS_URL}}

# OpenWebUI Authentication Settings
WEBUI_AUTH=false
WEBUI_AUTH_TRUSTED_EMAIL_HEADER=

# OpenWebUI User Settings
ENABLE_SIGNUP=true
DEFAULT_USER_ROLE=user

# Logging Configuration
WEBUI_LOG_LEVEL=INFO

# OpenWebUI Features
ENABLE_OAUTH=false
ENABLE_API_KEY_GENERATION=true

# OpenAI Configuration
# Additional OpenAI API configuration if needed
# OPENAI_API_ORGANIZATION=

# Session Configuration
SESSION_COOKIE_NAME=webui-session
SESSION_COOKIE_SECURE=false
SESSION_COOKIE_SAMESITE=Lax

# RAG Configuration
RAG_EMBEDDING_MODEL=nomic-embed-text
RAG_RERANKING_MODEL=

# Advanced Settings
WEBUI_SECRET_KEY=your_secret_key_here
WEBUI_BRANDING=
WEBUI_NAME=OpenWebUI

# OpenWebUI Database Settings
# Required for persistent storage
ENABLE_QDRANT_EMBED_MODELS=false
VECTOR_DB=

# Ollama Integration (if using local models)
# OLLAMA_API_BASE_URL=
# OLLAMA_EMBEDDING_MODEL=
